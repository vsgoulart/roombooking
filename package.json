{
  "name": "roombooking",
  "version": "1.0.0",
  "description": "1aim room booking challenge",
  "scripts": {
    "analyze": "bnr analyze",
    "build": "bnr build",
    "clean": "bnr clean",
    "serve": "bnr serve",
    "test": "bnr test"
  },
  "betterScripts": {
    "analyze": {
      "command": "bnr clean && webpack --env.prod --env.analyze -p",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build": {
      "command": "bnr clean && webpack --env.prod -p",
      "env": {
        "NODE_ENV": "production",
        "PUBLIC_PATH": "/"
      }
    },
    "clean": {
      "command": "rimraf dist"
    },
    "serve": {
      "command": "webpack-dev-server",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "test": {
      "command": "jest",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vsgoulart/roombooking.git"
  },
  "author": "Vin√≠cius Goulart <goulart.vs@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "better-npm-run": "^0.1.0",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "1.x",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.2",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.5",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.1",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "postcss": "^6.0.13",
    "postcss-hexrgba": "^1.0.0",
    "postcss-loader": "^2.0.8",
    "precss": "^2.0.0",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.9.3"
  }
}
